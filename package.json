{
  "name": "storybookwithvite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "t": "jest --json --outputFile=button.testResults.json",
    "test": "npx jest src",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@popperjs/core": "^2.11.6",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-jest": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/svelte": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/vite-plugin-svelte": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@testing-library/user-event": "^14.4.3",
    "@toast-ui/chart": "^4.6.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.2.5",
    "@types/plotly.js-basic-dist": "^1.54.1",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^29.3.1",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "canvas": "^2.11.0",
    "chart.js": "^4.2.0",
    "chartjs-adapter-moment": "^1.0.1",
    "chartjs-plugin-zoom": "^2.0.0",
    "classnames": "^2.3.2",
    "d3": "^7.8.2",
    "d3-force": "^3.0.0",
    "d3fc": "^15.2.6",
    "flowbite": "^1.6.3",
    "flowbite-svelte": "^0.29.12",
    "highcharts": "^10.3.3",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-transform-stub": "^2.0.0",
    "jest-webgl-canvas-mock": "^0.2.3",
    "moment": "^2.29.4",
    "pixi-viewport": "^4.38.0",
    "pixi.js": "^7.1.2",
    "plotly.js-basic-dist": "^2.18.2",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "svelte": "^3.55.0",
    "svelte-chartjs": "^3.1.2",
    "svelte-check": "^2.10.0",
    "svelte-jester": "^2.3.2",
    "svelte-loader": "^3.1.4",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.5",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "uuid": "^9.0.0",
    "vite": "^4.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2|svg)$": "jest-transform-stub"
    },
    "moduleFileExtensions": [
      "svelte",
      "js",
      "ts"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "setupFiles": [
      "jest-canvas-mock",
      "jest-webgl-canvas-mock"
    ]
  }
}
